	<xs:simpleType name="type1153CodeList">
		<xs:annotation>
			<xs:documentation xml:lang="en">Code list for Reference code qualifier (B.4.1153)</xs:documentation>
			<xs:appinfo>
				<dc:title>Reference code qualifier</dc:title>
				<dc:source>B.4.1153</dc:source>
				<dcterms:rightsHolder>UN/EDIFACT</dcterms:rightsHolder>
				<dc:source>UNTDID/UNCL</dc:source>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
			<xs:enumeration value="AGX">
