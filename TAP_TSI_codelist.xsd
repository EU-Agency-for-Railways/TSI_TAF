					<xs:documentation xml:lang="en" doc:docType="Remark">Also: Date and time of creation</xs:documentation>
					<xs:documentation xml:lang="en" doc:docType="Status">0</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="62">
				<xs:annotation>
					<xs:documentation xml:lang="en">Excluded date</xs:documentation>
