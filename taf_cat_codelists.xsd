					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="1014">
					<xs:annotation>
						<xs:documentation>New Route</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="1015">
					<xs:annotation>
						<xs:documentation>Updated Route</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="DE01">
					<xs:annotation>
						<xs:documentation>Reference to an associated empty/transfer train</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="DE02">
					<xs:annotation>
						<xs:documentation>Reference to an associated main run</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="DE03">
					<xs:annotation>
